{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "core:window:allow-show",
    "opener:default",
    "clipboard-manager:allow-write-image",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-load",
    "dialog:allow-save",
    "dialog:allow-open",
    "fs:allow-write-text-file",
    "fs:allow-read"
  ]
}
